<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.0/jquery.min.js"></script>
<script type="text/javascript">
$(function(){
	$("div").append(localStorage.getItem(name));
	
	$("#out").click(function(){
		localStorage.removeItem(name);
		$("div").childNodes[2].remove();
	})

})


</script>
